{
  "name": "stateset-response-cli",
  "version": "1.2.0",
  "description": "CLI tool for managing the StateSet Response platform via AI agent",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "response": "./bin/stateset-response",
    "response-whatsapp": "./bin/stateset-whatsapp",
    "response-slack": "./bin/stateset-slack",
    "response-gateway": "./bin/stateset-gateway"
  },
  "main": "dist/cli.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results.xml"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "croner": "^9.0.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "inquirer": "^9.3.0",
    "ora": "^8.1.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.0"
  },
  "optionalDependencies": {
    "@slack/bolt": "^3.22.0",
    "@whiskeysockets/baileys": "^6.7.0",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^0.34.6",
    "tsx": "^4.19.0",
    "typescript": "^5.5.0",
    "vitest": "^0.34.6"
  }
}
